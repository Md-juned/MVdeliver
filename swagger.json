            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "id": {
                    "type": "integer",
                    "default": 0
                  },
                  "name": {
                    "type": "string",
                    "default": "Oreo Shake"
                  },
                  "slug": {
                    "type": "string",
                    "default": "oreo-shake",
                    "description": "Optional unique identifier"
                  },
                  "category_id": {
                    "type": "integer",
                    "default": 1
                  },
                  "restaurant_id": {
                    "type": "integer",
                    "default": 1
                  },
                  "price": {
                    "type": "number",
                    "default": 150
                  },
                  "offer_price": {
                    "type": "number",
                    "default": 60
                  },
                  "short_description": {
                    "type": "string",
                    "default": "Best Oreo shake"
                  },
                  "status": {
                    "type": "string",
                    "default": "active"
                  },
                  "image": {
                    "type": "string",
                    "format": "binary"
                  },
                  "sizes": {
                    "type": "string",
                    "example": "[{\"size_name\":\"Large\",\"price\":80},{\"size_name\":\"Medium\",\"price\":120}]",
                    "description": "JSON array of size objects"
                  },
                  "specifications": {
                    "type": "string",
                    "example": "[\"4 Chicken\",\"Spicy\"]",
                    "description": "JSON array of specification strings"
                  },
                  "addon_ids": {
                    "type": "string",
                    "example": "[1,5]",
                    "description": "JSON array of addon IDs"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Request successful"
          },
          "400": {
            "description": "Bad request"
          },
          "401": {
            "description": "Unauthorized"
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
